{
    "name": "avtocod/b2b-api-php-laravel",
    "description": "Laravel package for a working with B2B API service",
    "keywords": [
        "laravel",
        "b2b",
        "service",
        "avtocod"
    ],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Avtocod",
            "homepage": "https://github.com/avtocod"
        }
    ],
    "require": {
        "php": "^5.6 || >=7.0 <7.3",
        "avto-dev/b2b-api-php-laravel": "2.*",
        "laravel/framework": "5.4.* || 5.5.* || 5.6.*"
    },
    "require-dev": {
        "laravel/laravel": "5.4.* || 5.5.* || 5.6.*",
        "mockery/mockery": "0.9.*",
        "phpunit/phpunit": "~5.7 || ^6.4"
    },
    "autoload": {
        "psr-4": {
            "Avtocod\\B2BApiLaravel\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Avtocod\\B2BApiLaravel\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "scripts": {
        "test": "./vendor/bin/phpunit"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Avtocod\\B2BApiLaravel\\B2BApiServiceProvider"
            ]
        }
    },
    "support": {
        "issues": "https://github.com/avtocod/b2b-api-php-laravel/issues",
        "source": "https://github.com/avtocod/b2b-api-php-laravel"
    }
}
